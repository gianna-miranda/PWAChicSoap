    "concurrently": "^4.1.0",
    "nodemon": "^1.19.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt-nodejs": "0.0.3",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.3",
    "helmet": "^3.21.1",
    "if-env": "^1.0.4",
    "inquirer": "^7.0.0",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.11",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "node": "^12.10.0",
    "request": "^2.88.0"
  }
